{
    "name": "metal-archives-js",
    "version": "0.0.0",
    "description": "The unofficial Metal Archives client for Node.js",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "prepublish": "npm run build",
        "lint": "tslint --project tsconfig.json --type-check",
        "build": "tsc",
        "test": "nyc mocha"
    },
    "nyc": {
        "include": ["lib/**/*.ts"],
        "extension": [".ts"],
        "require": ["ts-node/register"],
        "reporter": ["text-summary", "html"],
        "sourceMap": true,
        "instrument": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nrlquaker/metal-archives-js.git"
    },
    "keywords": ["metal", "archives", "rest", "api", "client"],
    "author": "nrlquaker",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nrlquaker/metal-archives-js/issues"
    },
    "homepage": "https://github.com/nrlquaker/metal-archives-js#readme",
    "dependencies": {
        "cheerio": "^1.0.0-rc.2",
        "request": "^2.83.0"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/cheerio": "^0.22.7",
        "@types/mocha": "^2.2.48",
        "@types/request": "^2.47.0",
        "chai": "^4.1.2",
        "mocha": "^5.0.1",
        "nyc": "^11.4.1",
        "source-map-support": "^0.5.3",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "typescript": "^2.7.2"
    }
}
